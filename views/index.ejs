<script type="text/javascript">
// Load google charts
google.charts.load('current', {'packages':['corechart']});
google.charts.setOnLoadCallback(drawChart);

// Draw the chart and set the chart values
function drawChart() {
  var temp  = document.getElementById("data1").value;
  var humidity =  document.getElementById("data2").value;
  var pm2 =  document.getElementById("data3").value;
  var co =  document.getElementById("data4").value;
  var lpg  =  document.getElementById("data5").value;

  var da= google.visualization.arrayToDataTable([
  ['Task', 'Hours per Day'],
  ['PM 2.5', parseInt(pm2)],
  ['CO', parseInt(co)],
  ['LPG Gas Detection', parseInt(lpg)],
  ['Fresh Air', 10000],

]);

// console.log(data);
// console.log(d);
  // Optional; add a title and set the width and height of the chart
  var options = {'title':'Air Quality Indexing', 'width':900, 'height':600};

  // Display the chart inside the <div> element with id="piechart"
  var chart = new google.visualization.PieChart(document.getElementById('piechart'));
  chart.draw(da, options);
}

var textWrapper = document.querySelector('.ml3');
textWrapper.innerHTML = textWrapper.textContent.replace(/\S/g, "<span class='letter'>$&</span>");

anime.timeline({loop: true})
  .add({
    targets: '.ml3 .letter',
    opacity: [0,1],
    easing: "easeInOutQuad",
    duration: 2250,
    delay: (el, i) => 150 * (i+1)
  }).add({
    targets: '.ml3',
    opacity: 0,
    duration: 1000,
    easing: "easeOutExpo",
    delay: 1000
  });
</script>
