   <table>
     <tr>
       <th style="text-align: center;">Date, Time (Data Recorded)</th>
       <th style="text-align: center;">LPG Gas Level reading</th>
     </tr>
        
    <% for(var i = data.length-1 ; i >= 0 ; i-- ) { %>
    
        <% var date="" %>
      <% for(var j = 0 ; j < 10; j++){%>
          <% date = date + data[i].created_at[j] %>
      <% }  %>
      
        <% var time="" %>
        <% for(var j = 11 ; j < 19; j++){%>
            <% time = time + data[i].created_at[j] %>
        <% } %>
    <tr>
        <td style="text-align: center;"><%= time%> , <%= date %> </td>
       <td style="text-align: center;"> <%= data[i].field5 %></td>
    </tr>
    
     <% } %>
 
  </table>
